<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginStart="8dp"
    android:layout_marginTop="4dp"
    android:layout_marginEnd="8dp"
    android:layout_marginBottom="4dp"
    android:background="?android:attr/selectableItemBackground"
    app:cardElevation="4dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:layout_centerVertical="true"
        android:layout_toStartOf="@+id/layout_meta"
        app:layout_constraintBottom_toTopOf="@+id/button6"
        app:layout_constraintTop_toTopOf="@+id/button6">

        <!--        <ImageView-->
        <!--            android:id="@+id/item_priority_level"-->
        <!--            android:layout_width="29dp"-->
        <!--            android:layout_height="34dp"-->
        <!--            android:layout_marginStart="8dp"-->
        <!--            android:layout_marginTop="8dp"-->
        <!--            android:layout_marginEnd="223dp"-->
        <!--            android:contentDescription="@string/priority_level"-->
        <!--            android:src="@drawable/ic_priority_high"-->
        <!--            app:layout_constraintEnd_toStartOf="@+id/button6"-->
        <!--            app:layout_constraintStart_toStartOf="parent"-->
        <!--            app:layout_constraintTop_toTopOf="parent" />-->


        <TextView
            android:id="@+id/item_tv_score"
            android:layout_width="225dp"
            android:layout_height="0dp"
            android:layout_gravity="center_vertical"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="12dp"

            android:layout_marginBottom="17dp"
            android:fontFamily="@font/montserrat_medium"
            android:text="stress score"
            android:textAppearance="@style/TextAppearance.MaterialComponents.Subtitle1"
            android:textSize="15sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/item_tv_start_time_date"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/item_tv_start_time_date"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_gravity="center_vertical"
            android:layout_marginEnd="73dp"
            android:textStyle="bold"
            android:layout_marginBottom="4dp"
            android:fontFamily="@font/montserrat_medium"
            android:textAppearance="@style/TextAppearance.MaterialComponents.Subtitle2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/level_s"
            app:layout_constraintTop_toBottomOf="@+id/item_tv_score"
            tools:text="22:22" />

        <TextView
            android:id="@+id/level_s"
            android:layout_width="0dp"
            android:layout_height="30dp"
            android:layout_marginStart="4dp"
            android:layout_marginEnd="1dp"
            android:layout_marginBottom="4dp"
            android:fontFamily="@font/montserrat_medium"

            android:text="Check -in"
            android:textColor="@color/black"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/item_tv_start_time_date"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/level_s2"
            android:layout_width="153dp"
            android:layout_height="30dp"
            android:layout_marginStart="4dp"
            android:layout_marginBottom="56dp"
            android:fontFamily="@font/montserrat_medium"
            android:text="Your Score"
            android:textColor="@color/black"
            android:textSize="15sp"

            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />


